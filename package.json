{
  "name": "angular-webpack-workflow",
  "version": "0.0.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "postinstall": "tsd install",
    "prebuild": "npm install",
    "build": "webpack --config config/webpack/webpack.build.js --bail -p",
    "serve": "webpack-dev-server --config config/webpack/webpack.serve.js --history-api-fallback --inline --progress --hot",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eneuhauser/angular-webpack-workflow.git"
  },
  "author": "Eric Neuhauser",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "devDependencies": {
    "angular-mocks": "^1.4.7",
    "autoprefixer": "^6.1.0",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "baggage-loader": "^0.2.4",
    "clean-webpack-plugin": "^0.1.4",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "glob": "^6.0.1",
    "html-loader": "^0.3.0",
    "isparta-instrumenter-loader": "^0.2.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.22",
    "karma-webpack": "^1.7.0",
    "ngtemplate-loader": "^1.3.1",
    "loader-utils": "^0.2.11",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.18",
    "postcss-loader": "^0.8.0",
    "sass-loader": "^3.1.1",
    "source-map": "^0.5.3",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.1",
    "tsd": "^0.6.5",
    "typescript": "^1.6.2",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.5",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "angular": "^1.4.7"
  }
}
